{
    "name": "codescope",
    "displayName": "CodeScope",
    "description": "Inject answers directly into any file from Stack Overflow with one simple CodeScope command...",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexhaislip/CodeScope_Visual_Studio_Extension"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
